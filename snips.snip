	{trigger: "^", replacement: "( ${VISUAL} )^{$0}$1", options: "mA"},
	{trigger: "J", replacement: "\\conj{${VISUAL}$0}$1", options: "mA"},
    {trigger: "A", replacement: "\\begin{align}\n ${VISUAL} $0 \n \\end{align}", options: "mA"},
	{trigger: "T", replacement: "\\left. ${VISUAL} \\right|_{${0:x=0}}", options: "mA"},

