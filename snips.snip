    {trigger: "orr", replacement: "\\cup", options: "mA"},
    {trigger: "inn", replacement: "\\in $0", options: "mA"},
    {trigger: "nin", replacement: "\\notin $0", options: "mA"}, 
	{trigger: "!inn", replacement: "\\notin $0", options: "mA"}, 
	{trigger: "\\notinn", replacement: "n \\in", options: "mA"}, 
	{trigger: "\\xi nn", replacement: "x \\in $0", options: "mA"}, 
	{trigger: "sub", replacement: "\\subset", options: "mA"},
    {trigger: "\\subset eq", replacement: "\\subseteq", options: "mA"},
    {trigger: "set", replacement: "\\{ $0 \\}$1", options: "mA"},
    {trigger: "=>", replacement: "\\Rightarrow $0", options: "mA"},
    {trigger: "=<", replacement: "\\Leftarrow ", options: "mA"},
    {trigger: "iff", replacement: "\\Leftrightarrow", options: "mA"},
    {trigger: "e\\xi sts", replacement: "\\exists", options: "mA", priority: 1},
	{trigger: "!E", replacement: "\\exists ", options: "mA", priority: 1},
    {trigger: "==", replacement: "\\equiv ", options: "mA"},
	{trigger: "=~", replacement: "\\cong", options: "mA"},
